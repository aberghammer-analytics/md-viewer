{
  "$schema": "https://schemas.tauri.app/capability/2",
  "identifier": "default",
  "description": "Default capabilities for the markdown viewer",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "core:window:default",
    "core:window:allow-close",
    "core:window:allow-destroy",
    "core:window:allow-set-title",
    "dialog:default",
    "dialog:allow-confirm",
    "dialog:allow-save"
  ]
}
